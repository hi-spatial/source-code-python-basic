   "source": [
    "centroid = lambda points: (sum([p[0] for p in points]) / len(points), sum([p[1] for p in points]) / len(points))\n",
    "\n",
    "points = [(1, 2), (3, 4), (5, 6)]\n",
    "result = centroid(points)\n",
    "print(result)\n",
