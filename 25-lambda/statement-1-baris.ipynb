   "source": [
    "polygon_area = lambda coords: 0.5 * abs(sum(x0*y1 - x1*y0 for (x0, y0), (x1, y1) in zip(coords, coords[1:] + [coords[0]])))\n",
    "\n",
    "coords = [(0, 0), (4, 0), (4, 3)]\n",
    "result = polygon_area(coords)\n",
    "print(result)\n",
    "# output âžœ 6.0"
