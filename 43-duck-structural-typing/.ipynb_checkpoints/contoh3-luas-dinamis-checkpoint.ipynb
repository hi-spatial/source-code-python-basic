    "import random\n",
    "\n",
    "def do_the_math(obj):\n",
    "    area = obj.calculate_area()\n",
    "    print(f\"Luas dari {type(obj).__name__}: {area}\")\n",
    "\n",
    "class PlotAcak:\n",
    "    def __init__(self):\n",
    "        self.calculate_area = lambda: random.randint(1, 10) * 10\n",
    "\n",
    "plot = PlotAcak()\n",
    "do_the_math(plot)"
