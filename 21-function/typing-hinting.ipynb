    "def hitung_luas_lingkaran(r: int) -> float:\n",
    "    luas = 3.14 * (r ** 2)\n",
    "    print(f\"Luas lingkaran dengan radius {r} adalah {luas:.2f}\")\n",
    "    return luas\n",
    "\n",
    "# Memanggil fungsi\n",
    "hitung_luas_lingkaran(5)\n"
