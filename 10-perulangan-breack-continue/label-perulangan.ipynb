   "source": [
    "max = int(input(\"Jumlah bintang: \"))\n",
    "outer_loop = True\n",
    "\n",
    "for i in range(max):\n",
    "    if not outer_loop:\n",
    "        break\n",
    "\n",
    "    for j in range(i + 1):\n",
    "        print(\"*\", end=\" \")\n",
    "        if j >= 7:\n",
    "            outer_loop = False\n",
    "            break\n",
    "    print()"
   ]
