    "class Location:\n",
    "    def __init__(self, name, lat, lon):\n",
    "        self.name = name\n",
    "        self.lat = lat\n",
    "        self.lon = lon\n",
    "\n",
    "loc = Location(\"Malang\", -7.98, 112.63)\n",
    "print(loc.name, loc.lat, loc.lon)\n",
    "\n",
    "del loc.lon  # Menghapus atribut longitude"
   ]
