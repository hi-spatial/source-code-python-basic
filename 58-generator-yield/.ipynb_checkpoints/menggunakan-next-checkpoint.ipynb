    "    yield \"hello python\"\n",
    "    yield \"how are you\"\n",
    "    yield \"nice to meet you\"\n",
    "\n",
    "messages = random_messages()\n",
    "\n",
    "gen = random_messages()\n",
    "\n",
    "print(next(gen))  # Output: hello python\n",
