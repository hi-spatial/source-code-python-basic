   "source": [
    "def infinite_counter():\n",
    "    i = 0\n",
    "    while True:\n",
    "        yield i\n",
    "        i += 1\n",
    "\n",
    "counter = infinite_counter()\n",
    "print(next(counter))  # Output: 0\n",
    "print(next(counter))  # Output: 1"
   ]
