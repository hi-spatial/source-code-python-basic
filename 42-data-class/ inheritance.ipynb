   "source": [
    "from dataclasses import dataclass\n",
    "\n",
    "@dataclass\n",
    "class Shape:\n",
    "    name: str\n",
    "\n",
    "@dataclass\n",
    "class Polygon(Shape):\n",
    "    sides: int\n",
    "\n",
    "square = Polygon(name=\"Square\", sides=4)\n",
    "print(f\"{square.name} memiliki {square.sides} sisi\")"
   ]
